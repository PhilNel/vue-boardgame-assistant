{
  "name": "boardgame-wiz",
  "version": "1.0.0",
  "description": "Boardgame Wiz - Your magical AI assistant for board game rules and gameplay",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:with-types": "vue-tsc && vite build",
    "build:prod": "NODE_ENV=production vite build",
    "build:s3": "bun scripts/build-for-s3.js",
    "deploy:s3": "bun scripts/deploy-to-s3.js",
    "build-and-deploy": "bun run build:s3 && bun run deploy:s3",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@vueuse/core": "^10.7.0",
    "axios": "^1.10.0",
    "pinia": "^3.0.3",
    "uuid": "^9.0.1",
    "vue": "^3.4.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/uuid": "^9.0.7",
    "@vitejs/plugin-vue": "^5.0.0",
    "terser": "^5.43.1",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vue-tsc": "^1.8.25"
  }
}
